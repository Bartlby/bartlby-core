CC = @echo "[CC] $<";@CC@
LNK = @echo "[LNK] $<"; @CC@
CPPVA = @INSTALL_PATH@ @INSTALL_OPTIONS@
RMVFR = rm -vfr
MKDIRP = @INSTALL_PATH@ @INSTALL_OPTIONS@ -d
EXTRAOPTIONS = -fPIC  -g -Wall @LDFLAGS@ @SNMP_ADDON@ @OPENSSL_ADDON@ @NRPE_ADDON@
INCLUDE_L = -I../include/
INCLUDE_LL = -I../../include/
INCLUDE_MYSQL = @INC_MYSQL@
DYNLINK = -fPIC -shared
LMYSQL = @LIB_MYSQL@
LDL = @LD_DL@
CHMOD = chmod
SNMP_ADDON = @SNMP_FLAG@ @SNMP_LIBDIR@ @SNMP_INCDIR@ 
OPENSSL_ADDON = @OPENSSL_FLAG@ @OPENSSL_LIBDIR@ @OPENSSL_INCDIR@
BARTLBY_HOME = @prefix@
